{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:node/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error",
        "no-underscore-dangle": "off",
        "no-console":"off",
        "func-names": "off",
        "import/no-unresolved": "off",
        "node/no-missing-require": "off",
        "consistent-return": "off"
    }
}
